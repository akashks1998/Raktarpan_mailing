<!DOCTYPE html>
<html lang="en">

<head>
    <title>Login V6</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/css/util.css">
    <link rel="stylesheet" type="text/css" href="/css/main.css">
    <!--===============================================================================================-->
</head>

<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-t-85 p-b-20 ">
                <form class="login100-form validate-form" method="POST" action="<%= id %>/deadline" style="margin-bottom:70px;">
                    <span class="login100-form-title p-b-70">
                        Add Deadline task
                    </span>
                    <span class="login100-form-avatar">
                        <img src="/images/logo.png" alt="AVATAR">
                    </span>


                    <div class="wrap-input100 validate-input m-t-85 m-b-35" data-validate="Enter Event name">
                        <input class="input100" type="text" name="name">
                        <span class="focus-input100" data-placeholder="Event name"></span>
                    </div>

                    <div class="wrap-input100 validate-input  m-b-50" data-validate="Enter Deadline">
                        <!-- <input class="input100" type="text" name="user"> -->
                        <input type="datetime-local" class="input100" value='<%= start %>' placeholder="" name="deadline">
                        <span class="focus-input100" data-placeholder="Deadline"></span>
                    </div>

                    <div class="wrap-input100 validate-input  m-b-50" data-validate="Enter Hours required">
                        <input class="input100" type="number" name="hours">
                        <span class="focus-input100" data-placeholder="Hours"></span>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            Add task
                        </button>
                    </div>
                </form>
                <hr class="m-0">
                <form class="login100-form validate-form" method="POST" action="<%= id %>/fixed">
                    <span class="login100-form-title p-b-70">
                        Add Fixed time task
                    </span>


                    <div class="wrap-input100 validate-input m-t-85 m-b-35" data-validate="Enter Event name">
                        <input class="input100" type="text" name="name">
                        <span class="focus-input100" data-placeholder="Event name"></span>
                    </div>

                    <div class="wrap-input100 validate-input  m-b-50" data-validate="Enter Start time">
                        <!-- <input class="input100" type="text" name="user"> -->
                        <input type="datetime-local" value='<%= start %>' class="input100" name="start">

                        <span class="focus-input100" data-placeholder="Start time"></span>
                    </div>

                    <div class="wrap-input100 validate-input  m-b-50" data-validate="Enter end date">
                        <input type="datetime-local" value='<%= start %>' class="input100" name="end">
                        <span class="focus-input100" data-placeholder="End time"></span>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            Add task
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="/vendor/bootstrap/js/popper.js"></script>
    <script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="/vendor/daterangepicker/moment.min.js"></script>
    <script src="/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="/js/main.js"></script>

</body>

</html>